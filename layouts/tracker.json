{
	"shared_item_grid": {
		"type": "array",
		"orientation": "vertical",
		"margin": "0,0",
		"content": [
		{
			"type": "array",
			"orientation": "horizontal",
			"margin": "0,0",
			"content": [
			{
				"type": "itemgrid",
				"item_margin": "6,1",
				"h_alignment": "left",
				"rows": [
					[ "charge", "diffusion", "wide", "super", "ice", "storm" ],
					[ "plasma", "wave", "grapple", "cloak", "flash", "pulse" ],
					[ "morph", "bomb", "cross", "pbomb", "varia", "gravity" ],
					[ "magnet", "speed", "spin", "space", "screw", "thex" ],
					[ "corpius", "kraid", "drogyga", "escue", "z57", "golzuna" ],
					[ "emmi2", "emmi3", "emmi4", "emmi5", "emmi6", "emmi7" ]
				]
			},
			{
				"type": "array",
				"orientation": "vertical",
				"v_alignment": "center",
				"margin": "5,0,0,0",
				"content": [
				{
					"type": "itemgrid",
					"margin": "0,0",
					"item_margin": "2,0",
					"h_alignment": "center",
					"rows": [
						[ "", "" ]
					]
				},
				{
					"type": "itemgrid",
					"margin": "0,4,0,0",
					"item_margin": "2,0",
					"h_alignment": "center",
					"rows": [
						[ "", "" ]
					]
				},
				{
					"type": "item",
					"item": "difficulty",
					"h_alignment": "center",
					"margin": "0,16,0,0"
				}
				]
			}
			]
		}
		]
	},
	
	"tracker_capture_item": {
		"type": "container",
		"content": {
			"type": "layout",
			"key": "shared_item_grid"
		}
	},
	
